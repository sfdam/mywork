public  class openFilteredReportController {

    @AuraEnabled
    public static Contact getContactInfo() {

        Contact c;

        Id profileId=userinfo.getProfileId();

        String profileName=[SELECT Id,Name FROM Profile where Id=:profileId].Name;

        if(!(profileName == 'System Administrator' ||  profileName == 'Amministratore di sistema')){

             c = [SELECT PTF_IsSuperuser__c FROM Contact WHERE PTF_User__c =: UserInfo.getUserId() LIMIT 1];
        }
        else {

             c = [SELECT PTF_IsSuperuser__c FROM Contact  LIMIT 1];
        }
        return c;
        
    }
    
    @AuraEnabled
    public static Map<String,sObject> getStrutturaBanca(){
        
        return   SV_Utilities.keyBy([ SELECT IDCed__c, Nome_Livello__c FROM Struttura_Banca__c], 'IDCed__c', false);
             
    }

    @AuraEnabled
    public static String getIdCedFiliale(String filialeId) {

        return [SELECT PTF_IdCED__c FROM  Account where id  =: filialeId LIMIT 1].PTF_IdCED__c;


    }

    @AuraEnabled
    public static Map<String,sObject> getReportName(){
        
        return SV_Utilities.keyBy([SELECT Id, Nome_Report__c,DeveloperName FROM Report_Super_User__mdt order by Nome_Report__c ASC ], 'DeveloperName', false);
    }

    @AuraEnabled
    public static string getReportId(String reportName){
        
        String newReportName = '%'+reportName+'%';
        
        return [SELECT Id FROM Report Where DeveloperName  LIKE: newReportName LIMIT 1 ].Id;
    }

    @AuraEnabled
    public static string getAccountId(String idCed ){

        return [SELECT Id, Name  FROM Account WHERE PTF_IdCED__c =: idCed LIMIT 1 ].Id;
        

    }
