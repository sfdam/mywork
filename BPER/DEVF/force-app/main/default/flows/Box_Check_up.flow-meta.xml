&quot;Check Up &quot; + 
IF(OR(TEXT({!Get_NDG.ModelloDiServizio__c}) = &apos;Business PMI&apos;, TEXT({!Get_NDG.ModelloDiServizio__c}) = &apos;Couco&apos;, TEXT({!Get_NDG.ModelloDiServizio__c}) = &apos;Key Client Imprese&apos;), &quot;Imprese&quot;, IF(TEXT({!Get_NDG.ModelloDiServizio__c}) = &apos;Personal&apos;, &quot;Personal&quot;, &quot;Private&quot;))
+  &quot; - &quot; + {!Get_NDG.Name}</expression>
    </formulas>
    <formulas>
        <name>Formula_SnapshotName</name>
        <dataType>String</dataType>
        <expression>/*IF(LEN(&quot;Check Up &quot; + IF(OR(TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Business PMI&apos;, TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Couco&apos;, TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Key Client Imprese&apos;), &quot;Imprese&quot;, IF(TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Personal&apos;, &quot;Personal&quot;, &quot;Private&quot;)) +  &quot; - &quot; + Get_NDG.Name)&gt; 80,
LEFT(&quot;Check Up &quot; + IF(OR(TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Business PMI&apos;, TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Couco&apos;), &quot;Imprese&quot;,  IF(TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Personal&apos;, &quot;Personal&quot;, &quot;Private&quot;)) +  &quot; - &quot; + Get_NDG.Name, 80),
&quot;Check Up &quot; + IF(OR(TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Business PMI&apos;, TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Couco&apos;), &quot;Imprese&quot;,  IF(TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Personal&apos;, &quot;Personal&quot;, &quot;Private&quot;)) +  &quot; - &quot; + Get_NDG.Name
) */
IF(LEN(&quot;Check Up &quot; + {!NameMDS} +  &quot; - &quot; + {!Get_NDG.Name})&gt; 80,
LEFT(&quot;Check Up &quot; + {!NameMDS} +  &quot; - &quot; + {!Get_NDG.Name}, 80),
&quot;Check Up &quot; + {!NameMDS} +  &quot; - &quot; + {!Get_NDG.Name}
)</expression>
    </formulas>
