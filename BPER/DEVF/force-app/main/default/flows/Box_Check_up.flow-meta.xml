    <formulas>
        <name>Formula_SnapshotFormula</name>
        <dataType>String</dataType>
        <expression>/* &quot;Snapshot Check Up &quot; + 
IF(Get_NDG.RecordType.DeveloperName = &quot;IndustriesBusiness&quot;, &quot;Imprese&quot;,  IF(TEXT(Get_NDG.ModelloDiServizio__c) = &apos;Personal&apos;, &quot;Personal&quot;, &quot;Private&quot;))
