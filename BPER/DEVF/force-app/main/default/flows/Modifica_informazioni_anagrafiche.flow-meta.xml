    <formulas>
        <name>Formula_CheckOre</name>
        <dataType>Boolean</dataType>
        <expression>IF (
VALUE({!Ora_inizio}) &lt; VALUE({!Ora_fIne}), TRUE,
IF (AND(VALUE({!Ora_inizio}) = VALUE({!Ora_fIne}), VALUE({!Minuti_inizio}) = VALUE({!Minuti_fine})),
VALUE({!Minuti_inizio}) &lt; VALUE({!Minuti_fine}), FALSE
)
)</expression>
    </formulas>
    <formulas>
