    <formulas>
        <name>DataEOraPresaAppuntamento</name>
        <dataType>String</dataType>
        <expression>IF(DAY(DATEVALUE({!ServiceAppointment.SchedStartTime})) &lt; 10, &apos;0&apos; + TEXT(DAY(DATEVALUE({!ServiceAppointment.SchedStartTime}))), TEXT(DAY(DATEVALUE({!ServiceAppointment.SchedStartTime})))) + &apos;/&apos; + 
IF(MONTH(DATEVALUE({!ServiceAppointment.SchedStartTime})) &lt; 10, &apos;0&apos; + TEXT(MONTH(DATEVALUE({!ServiceAppointment.SchedStartTime}))), TEXT(MONTH(DATEVALUE({!ServiceAppointment.SchedStartTime})))) + &apos;/&apos; + TEXT(YEAR(DATEVALUE({!ServiceAppointment.SchedStartTime}))) + &apos;, &apos; + MID(TEXT({!ServiceAppointment.SchedStartTime}),12,5) +  &apos;-&apos; + MID(TEXT({!ServiceAppointment.SchedEndTime}),12,5)</expression>
    </formulas>
    <formulas>
        <name>Formula_Cliente</name>
        <dataType>String</dataType>
        <expression>IF({!Account.RecordType.DeveloperName} = &apos;IndustriesBusiness&apos;, {!Account.Name}, {!Account.FirstName} + &quot; &quot; + {!Account.LastName})</expression>
    </formulas>
