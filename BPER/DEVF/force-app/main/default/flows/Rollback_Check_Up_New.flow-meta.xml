    <formulas>
        <name>Formula_RT_DevName</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!Get_Check_Up_corrente.CRM_Customer__r.ModelloDiServizio__c}) = &apos;Personal&apos;, &apos;Snapshot_Check_Up_Personal&apos;,
IF(TEXT({!Get_Check_Up_corrente.CRM_Customer__r.ModelloDiServizio__c}) = &apos;Consulenti Finanziari&apos;, &apos;Snapshot_Check_Up_Consulenti_Finanziari&apos;,
