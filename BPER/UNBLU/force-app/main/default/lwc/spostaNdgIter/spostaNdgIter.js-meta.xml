    <isExposed>true</isExposed>
    <targets>
        <target>lightning__RecordPage</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__RecordPage">
            <property name="profiliAutorizzati" type="String" label="Name Profili Autorizzati" description="definisci la lista dei profili autorizzati a visualizzare il componente, se vuoto tutti i profili sono autorizzati altrimenti vale la lista"/>
            <property name="titolo" type="String" label="Titolo" description="definisci il titolo del modale"/>
            <property name="labelBottone" type="String" label="Label Bottone" description="definisci la label del bottone"/>
            <property name="modelliDiServizio" type="String" label="Modelli di Servizio Ammessi" description="definisci i modelli di servizio ammessi"/>
            <property name="messaggioDiBloccoTitle" type="String" label="Messaggio di blocco" description="Messaggio di blocco"/>
            <property name="bloccoIter" type="Boolean" label="Blocca Creazione Iter" description="Blocca Creazione Iter"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>